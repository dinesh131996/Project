
public class Work{

public static void main (String [] args){
String s= "For the people by the people of the people";
String[] word = s.split();
Map<String, Integer> li = new HashMap<String,Integer>();
for(String kk : s){
if(li.containskey(kk)){
Integer int = li.get(kk);
li.put(kk, int+1);
}
else{
li.put(kk,1);
}
}
System.out.println(li);
}
}